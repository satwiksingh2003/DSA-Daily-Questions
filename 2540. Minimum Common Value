//Optimal Solution
class Solution {
    public int getCommon(int[] nums1, int[] nums2) {
        HashSet<Integer> set = new HashSet<>();

        for (int x : nums1) {
            set.add(x);
        }

        for (int y : nums2) {
            if (set.contains(y)) {
                return y;
            }
        }
        return -1;
    }
}

//Brute Force Method
/*import java.util.Arrays;

